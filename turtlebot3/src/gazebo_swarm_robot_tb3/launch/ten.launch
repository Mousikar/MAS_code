    </node>
    
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model $(arg robot_4_name) -x $(arg robot_4_x_pos) -y $(arg robot_4_y_pos) -z $(arg robot_4_z_pos) -Y $(arg robot_4_yaw) -param robot_description" />
 
