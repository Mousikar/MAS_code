    </node>
    
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model $(arg robot_6_name) -x $(arg robot_6_x_pos) -y $(arg robot_6_y_pos) -z $(arg robot_6_z_pos) -Y $(arg robot_6_yaw) -param robot_description" />
 
