    </node>
    
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model $(arg robot_5_name) -x $(arg robot_5_x_pos) -y $(arg robot_5_y_pos) -z $(arg robot_5_z_pos) -Y $(arg robot_5_yaw) -param robot_description" />
 
